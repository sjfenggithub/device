package com.yd.model;

import java.util.List;

public class SwitchBasicInfo {
	String IP;
	String dpid;
	String openflow;
	String tunnelPort;
	int virtualLink;
	List<SwitchPort> portList;
	public SwitchBasicInfo(String ip,String dpid,String openflow,String tunnelPort,int virtualLink,List<SwitchPort> portList){
		this.IP=ip;
		this.dpid=dpid;
		this.openflow=openflow;
		this.tunnelPort=tunnelPort;
		this.virtualLink=virtualLink;
		this.portList=portList;
		
	}
	public String getIp() {
		return IP;
	}
	public void setIp(String ip) {
		this.IP = ip;
	}
	public String getDpid() {
		return dpid;
	}
	public void setDpid(String dpid) {
		this.dpid = dpid;
	}
	public String getOpenfolw() {
		return openflow;
	}
	public void setOpenfolw(String openfolw) {
		this.openflow = openfolw;
	}
	public String getTunnelPort() {
		return tunnelPort;
	}
	public void setTunnelPort(String tunnelPort) {
		this.tunnelPort = tunnelPort;
	}
	public int getVirtualLink() {
		return virtualLink;
	}
	public void setVirtualLink(int virtualLink) {
		this.virtualLink = virtualLink;
	}
	public List<SwitchPort> getPortList() {
		return portList;
	}
	public void setPortList(List<SwitchPort> portList) {
		this.portList = portList;
	}
	
}
